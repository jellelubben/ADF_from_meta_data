{
	"name": "parquet_storage",
	"properties": {
		"parameters": {
			"filename": {
				"type": "string"
			},
			"dir": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().dir",
					"type": "Expression"
				},
				"container": "exports"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}